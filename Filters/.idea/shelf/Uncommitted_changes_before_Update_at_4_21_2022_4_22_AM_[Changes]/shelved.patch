ally\r\n    for (int i = 0; i < SIZE; ++i) {\r\n        for (int j = 0; j < SIZE; j++) {\r\n            for (int k = 0; k < 8; ++k) {\r\n                sum += img[i][j + k];\r\n            }\r\n            avg = sum/8;\r\n            img[i][j] = avg;\r\n            sum = 0;\r\n        }\r\n\r\n    }\r\n    sum = 0;\r\n    // Calculates average of each 8 subsequent pixels and assigns this value to each pixel of the 8 vertically\r\n    for (int i = 0; i < SIZE; ++i) {\r\n        for (int j = 0; j < SIZE; j++) {\r\n            for (int k = 0; k < 8; ++k) {\r\n                sum += img[j + k][i];\r\n            }\r\n            avg = sum/8;\r\n            img[j][i] = avg;\r\n            sum = 0;\r\n        }\r\n    }\r\n}
